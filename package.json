    "ng": "ng",
    "oldStart": "ng serve",
    "oldBuild": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "hbs": "~4.0.1",
    "jquery": "^3.2.1",
    "lodash": "~4.17.4",
    "morgan": "~1.8.1",
    "mysql": "^2.13.0",
    "nconf": "^0.8.4",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.1.0",
    "serve-favicon": "~2.4.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "~6.0.60",
    "del-cli": "^0.2.0",
    "html-loader": "^0.4.4",
