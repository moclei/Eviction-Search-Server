    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "raw-loader": "^0.5.1",
    "proxyquire": "~1.7.11",
    "webpack": "^2.6.1",
    "webpack-merge": "^4.1.0"
  }
